<settings>
    <profiles>
        <profile>
            <id>localdev</id>
            <repositories>
                <repository>
                    <id>lfinternal</id>
                    <url>https://maven.livefyre.com/content/groups/internal</url>
                </repository>
            </repositories>
        </profile>
    </profiles>
    <activeProfiles>
        <activeProfile>localdev</activeProfile>
    </activeProfiles>
    <mirrors>
        <mirror>
            <id>lfcentral</id>
            <url>https://maven.livefyre.com/content/groups/public/</url>
            <mirrorOf>central</mirrorOf>
        </mirror>
    </mirrors>
    <servers>
        <server>
            <id>lfpublic</id>
            <username>livefyre</username>
            <password>${env.M2_PWD}</password>
        </server>
        <server>
            <id>lfadobe</id>
            <username>livefyre</username>
            <password>${env.M2_PWD}</password>
        </server>
        <server>
            <id>lfinternal</id>
            <username>livefyre</username>
            <password>${env.M2_PWD}</password>
        </server>
        <server>
            <id>lfsnap</id>
            <username>livefyre</username>
            <password>${env.M2_PWD}</password>
        </server>
    </servers>
</settings>
